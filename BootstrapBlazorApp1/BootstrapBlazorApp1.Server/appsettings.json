{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "WebsiteOptions": {
    "ServerUrl": "https://www.blazor.zone",
    "Themes": [
      {
        "key": "bootstrap",
        "name": "Bootstrap",
        "files": []
      },
      {
        "key": "bule",
        "name": "蓝色主题",
        "files": [
          "./css/blue.css"
        ]
      },
      {
        "key": "ant",
        "name": "Ant Design (完善中)",
        "files": [
          "./css/ant.css"
        ]
      },
      {
        "key": "layui",
        "name": "LayUI (完善中)",
        "files": [
          "./css/layui.css"
        ]
      },
      {
        "key": "devui",
        "name": "DevUI",
        "files": [
          "./css/devui.css"
        ]
      }
    ]
  },
  "AllowedHosts": "*",
  "BootstrapBlazorOptions": {
    "ToastDelay": 4000,
    "MessageDelay": 4000,
    "SwalDelay": 4000,
    "EnableErrorLogger": false,
    "FallbackCulture": "en",
    "SupportedCultures": [
      "zh-CN",
      "en-US"
    ],
    "TableSettings": {
      "CheckboxColumnWidth": 36
    },
    "IgnoreLocalizerMissing": true,
    "StepSettings": {
      "Short": "1",
      "Int": "1",
      "Long": "1",
      "Float": "0.1",
      "Double": "0.01",
      "Decimal": "0.01"
    },
    "DefaultCultureInfo": "zh-CN",
    "ConnectionStrings": {
      "Default": "Data Source=:memory:"
    }
  }
}
